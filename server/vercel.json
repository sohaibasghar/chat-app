{
    "version": 2,
    "builds": [
      {
        "src": "server/src/index.js",
        "use": "@vercel/node",
        "config": {
          "maxDuration": 60
        }
      }
    ],
    "routes": [
      {
        "src": "/(.*)",
        "dest": "server/src/index.js"
      }
    ],
    "rewrites": [
      {
        "source": "/(.*)",
        "destination": "/api"
      }
    ]
  }